@page "/plans/view/{id:int}"
@inject HttpClient _client
@inject NavigationManager _navManager


<h3>View</h3>

<FormComponent plan="plan" Disabled="true" />


@code {

    [Parameter] public int id { get; set; }
    Plan plan = new Plan();
    protected async override Task OnParametersSetAsync()
    {
        plan = await
    _client.GetFromJsonAsync<Plan>($"{Endpoints.PlansEndpoint}/{id}");
    }

}